{
  "name": "urlparser-simple",
  "version": "1.0.3",
  "private": false,
  "description": "Simple URI parser for HTTP(S) and (S)FTP",
  "main": "urlparser.js",
  "engines": {
    "node": ">=4.0.0"
  },
  "devDependencies": {
    "typescript": "3.7.2",
    "@types/jest": "^24.0.24",
    "@types/sinon": "^7.5.1",
    "chai": "^4.2.0",
    "jest": "^24.9.0",
    "sinon": "^7.5.0",
    "ts-jest": "^24.2.0",
    "tslint": "^5.20.1"
  },
  "scripts": {
